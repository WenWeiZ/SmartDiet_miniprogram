.top{
  background-color:rgb(60,190,140);
  width: 750rpx;
  height:80rpx;
  display: flex;
  padding: 20rpx;
  flex-direction: row;
  justify-content: left;
  align-items: center;
}
.contain{
  padding-left: 20rpx;
  padding-right: 20rpx;
  display: flex;
  flex-direction: column;
}

.item{
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 100rpx;
  width: 100%;
  border-bottom: 1px solid rgb(226, 235, 231);
}